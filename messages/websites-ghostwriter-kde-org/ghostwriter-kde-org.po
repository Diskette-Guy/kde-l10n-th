msgstr ""

#. +> trunk5
#: content/documentation.md:205
msgid "<kbd>Shift</kbd>+<kbd>Enter</kbd>"
msgstr ""

#. +> trunk5
#: content/documentation.md:206
msgid "Inserts a Markdown-style line break."
msgstr ""

#. +> trunk5
#: content/documentation.md:208
msgid "<kbd>Ctrl</kbd>+<kbd>Enter</kbd>"
msgstr ""

#. +> trunk5
#: content/documentation.md:209
msgid "Inserts a line break as usual, except the current indentation will not be honored. In other words, if you are typing on an indented line and press `Ctrl+Enter`, no indentation will be added to the new line. Note that the Ctrl modifier can be combined with the Shift modifier to produce both an unindented line and a Markdown-style line break."
msgstr ""

#. +> trunk5
#: content/documentation.md:211
msgid "<kbd>Ctrl</kbd>+<kbd>.</kbd>"
msgstr ""

#. +> trunk5
#: content/documentation.md:212
msgid "Turns the current line or the selected lines into a block quote."
msgstr ""

#. +> trunk5
#: content/documentation.md:214
msgid "<kbd>Ctrl</kbd>+<kbd>,</kbd>"
msgstr ""

#. +> trunk5
#: content/documentation.md:215
msgid "Removes any block quote formatting from the current line or the selected lines of text."
msgstr ""

