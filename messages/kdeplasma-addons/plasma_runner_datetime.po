#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:23
#, kde-format
msgctxt "Note this is a KRunner keyword"
msgid "date"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:24
#, kde-format
msgctxt "Note this is a KRunner keyword"
msgid "time"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:28
#, kde-format
msgctxt "words to specify a time in a time zone or to convert a time to a time zone, e.g. 'current time in Berlin' or '18:00 UTC to CET', separated by '|' (will be used as a regex)"
msgid "to|in|as|at"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:34
#, kde-format
msgid "Displays the current date"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:35
#, kde-format
msgid "Displays the current time"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:36 datetimerunner.cpp:38
#, kde-format
msgctxt "The <> and space are part of the example query"
msgid " <timezone>"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:37
#, kde-format
msgid "Displays the current date and difference to system date in a given timezone"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:39
#, kde-format
msgid "Displays the current time and difference to system time in a given timezone"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:40
#, kde-format
msgctxt "The <> and space are part of the example query"
msgid "<time> <timezone> in <timezone>"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:41
#, kde-format
msgid "Converts the time from the first timezone to the second timezone. If only one time zone is given, the other will be the system time zone. If no date or time is given, it will be the current date and time."
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:58
#, kde-format
msgid "Today's date is %1"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:74
#, kde-format
msgctxt "time difference between time zones, e.g. in Stockholm it's 1 calendar day later than in Brasilia"
msgid "%1 later"
msgstr ""

#. +> stable5
#: datetimerunner.cpp:63
#, kde-format
msgctxt "date difference between time zones, e.g. in Stockholm it's 1 calendar day later than in Brasilia"
msgid "%1 later"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:76
#, kde-format
msgctxt "date difference between time zones, e.g. in Brasilia it's 1 calendar day earlier than in Stockholm"
msgid "%1 earlier"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:78
#, kde-format
msgctxt "no date difference between time zones, e.g. in Stockholm it's the same calendar day as in Berlin"
msgid "no date difference"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:93
#, kde-format
msgid "Current time is %1"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:112 datetimerunner.cpp:206
#, kde-format
msgctxt "time difference with calendar date difference between time zones, e.g. 22:00 Brasilia time in Stockholm = 02:00 + 1 day, where %1 is the time and %2 is the days later"
msgid "%1 + %2"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:119 datetimerunner.cpp:213
#, kde-format
msgctxt "time difference with calendar date difference between time zones, e.g. 02:00 Stockholm time in Brasilia = 22:00 - 1 day, where %1 is the time and %2 is the days earlier"
msgid "%1 - %2"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:128 datetimerunner.cpp:223
#, kde-format
msgctxt "time difference between time zones, e.g. in Stockholm it's 4 hours later than in Brasilia"
msgid "%1 later"
msgstr ""

#. +> trunk6 stable6
#: datetimerunner.cpp:130 datetimerunner.cpp:225
#, kde-format
msgctxt "time difference between time zones, e.g. in Brasilia it's 4 hours earlier than in Stockholm"
msgid "%1 earlier"
msgstr ""

#. +> stable5
#: datetimerunner.cpp:105
#, kde-format
msgctxt "time difference between time zones, e.g. in Brasilia it's 4 hours ealier than in Stockholm"
msgid "%1 earlier"
msgstr ""

#. +> trunk6 stable6 stable5
#: datetimerunner.cpp:132 datetimerunner.cpp:227
#, kde-format
msgctxt "no time difference between time zones, e.g. in Stockholm it's the same time as in Berlin"
msgid "no time difference"
msgstr ""
