#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:1
msgid "The select menu in Krita."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:11
msgid "Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:16
msgid "Select Menu"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:19
msgid "Select All"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:21
msgid "Selects the whole layer. Shortcut :kbd:`Ctrl + A`"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:22
msgid "Deselect"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:24
msgid "Deselects everything (except for active Selection Mask). Shortcut :kbd:`Ctrl + Shift + A`"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:25
msgid "Reselect"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:27
msgid "Reselects the previously deselected selection. Shortcut :kbd:`Ctrl + Shift + D`"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:28
msgid "Invert Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:30
msgid "Inverts the selection. Shortcut :kbd:`Ctrl + Shift + I`"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:31
msgid "Edit Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:33
msgid "When a selection is active, this will switch the active layer to the global selection mask, and the active tool to the appropriate tool for editing the selection depending on whether it's :ref:`vector <shape_selection_tool>` or :ref:`raster <freehand_brush_tool>`."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:34
msgid "Convert to Vector Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:36
msgid "This converts a raster selection to a vector selection. Any layers of transparency there might have been are removed."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:37
msgid "Convert to Raster Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:39
msgid "This converts a vector selection to a raster selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:40
msgid "Convert Shapes to Vector Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:42
msgid "Convert a vector shape to a vector selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:43
msgid "Convert to Shape"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:45
msgid "Converts a vector selection to a vector shape."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:46
msgid "Display Selection"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:48
msgid "Display the selection. If turned off selections will be invisible. Shortcut :kbd:`Ctrl + H`"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:49
msgid "Show Global Selection Mask"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:51
msgid "Shows the global selection as a selection mask in the layers docker. This is necessary to be able to select it for painting on."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:52
msgid "Scale..."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:54
msgid "Scale the selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:55
msgid "Select from Color Range..."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:57
msgid "Select from a certain color range."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:58
msgid "Select Opaque"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:60
msgid "Select all opaque (non-transparent) pixels in the current active layer. If there's already a selection, this will add the new selection to the old one, allowing you to select the opaque pixels of multiple layers into one selection. Semi-transparent (or semi-opaque) pixels will be semi-selected."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:62
msgid "Select Opaque (Replace)"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:63
msgid "Only select the opaque pixels of the layer, regardless of selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:64
msgid "Select Opaque (Add)"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:65
msgid "Adds the new selection from opaque pixels to the current selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:66
msgid "Select Opaque (Subtract)"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:67
msgid "Subtracts the new selection from opaque pixels from the current selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:69
msgid "Select Opaque (Intersection)"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:69
msgid "Gets the intersection between the new selection from opaque pixels and the current selection."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:70
msgid "Feather Selection..."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:72
msgid "Feathering in design means to soften sharp borders. So this adds a soft border to the existing selection. Shortcut :kbd:`Shift + F6`"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:73
msgid "Grow Selection..."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:75
msgid "Make the selection a few pixels bigger."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:76
msgid "Shrink Selection..."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:78
msgid "Make the selection a few pixels smaller."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:79
msgid "Border Selection..."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:81
msgid "Take the current selection and remove the insides so you only have a border selected."
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:82
msgid "Smooth"
msgstr ""

#. +> trunk5
#: ../../reference_manual/main_menu/select_menu.rst:84
msgid "Make the selection a little smoother. This removes jiggle."
msgstr ""
