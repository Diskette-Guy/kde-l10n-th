#: ../../reference_manual/blending_modes/negative.rst:16
msgid "Negative"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:18
msgid "These are all blending modes which seem to make the image go negative."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:20
#: ../../reference_manual/blending_modes/negative.rst:24
msgid "Additive Subtractive"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:28
msgid "This blending mode is called \"Additive Subtractive\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:30
msgid "Subtracts the square root of the lower layer from the upper layer."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:35
msgid ".. image:: images/blending_modes/negative/Blending_modes_Additive_Subtractive_Sample_image_with_dots.png"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:35
msgid "Left: **Normal**. Right: **Additive Subtractive**."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:37
#: ../../reference_manual/blending_modes/negative.rst:41
msgid "Arcus Tangent"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:45
msgid "This blending mode is called \"Arcus Tangent\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:47
msgid "Divides the lower layer by the top. Then divides this by Pi. Then uses that in an Arc tangent function, and multiplies it by two."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:53
msgid ".. image:: images/blending_modes/negative/Blending_modes_Arcus_Tangent_Sample_image_with_dots.png"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:53
msgid "Left: **Normal**. Right: **Arcus Tangent**."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:55
#: ../../reference_manual/blending_modes/negative.rst:59
msgid "Difference"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:63
msgid "This blending mode is called \"Difference\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:65
msgid "Checks per pixel of which layer the pixel-value is highest/lowest, and then subtracts the lower value from the higher-value."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:71
msgid ".. image:: images/blending_modes/negative/Blending_modes_Difference_Sample_image_with_dots.png"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:71
msgid "Left: **Normal**. Right: **Difference**."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:73
#: ../../reference_manual/blending_modes/negative.rst:77
msgid "Equivalence"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:81
msgid "This blending mode is called \"Equivalence\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:83
msgid "Subtracts the underlying layer from the upper-layer. Then inverts that. Seems to produce the same result as :ref:`bm_difference`."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:89
msgid ".. image:: images/blending_modes/negative/Blending_modes_Equivalence_Sample_image_with_dots.png"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:89
msgid "Left: **Normal**. Right: **Equivalence**."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:91
#: ../../reference_manual/blending_modes/negative.rst:95
msgid "Exclusion"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:99
msgid "This blending mode is called \"Exclusion\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:101
msgid "This multiplies the two layers, adds the source, and then subtracts the multiple of two layers twice."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:106
msgid ".. image:: images/blending_modes/negative/Blending_modes_Exclusion_Sample_image_with_dots.png"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:106
msgid "Left: **Normal**. Right: **Exclusion**."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:108
#: ../../reference_manual/blending_modes/negative.rst:112
msgid "Negation"
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:116
msgid "This blending mode is called \"Negation\" in English."
msgstr ""

#. +> trunk5
#: ../../reference_manual/blending_modes/negative.rst:118
msgid "The absolute of the 1.0f value subtracted by the base subtracted by the blend layer. abs(1.0f - Base - Blend)"
msgstr ""

