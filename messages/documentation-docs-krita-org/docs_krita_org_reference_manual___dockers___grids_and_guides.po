msgid "Guides"
msgstr ""

#. +> trunk5
#: ../../reference_manual/dockers/grids_and_guides.rst:12
msgid "Grid"
msgstr ""

#. +> trunk5
#: ../../reference_manual/dockers/grids_and_guides.rst:12
msgid "Isometric Grid"
msgstr ""

#. +> trunk5
