#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:50
msgid "Alpha ATOP"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:52
msgid "Draw a pixel from the base layer on top of the blend layer if both layers have a non-alpha pixel (|frei0r.alphaatop|)"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:55
msgid "Alpha IN"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:57
msgid "Draw a pixel from the base layer only where both layers have a non-alpha pixel. This is the alpha equivalent of clipping. (|frei0r.alphain|)"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:60
msgid "Alpha OUT"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:62
msgid "Draw an alpha pixel only where both layers have a non-alpha pixel, otherwise draw the pixel from the base layer. This is the alpha equivalent of clipping. (|frei0r.alphaout|)"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:65
msgid "Alpha OVER"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:67
msgid "Overlay the blend layer with the base layer (|frei0r.alphaover|)"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:70
msgid "Alpha XOR"
msgstr ""

#. +> trunk5
#: ../../compositing/compositions/alpha_operations.rst:72
msgid "Draw a pixel only where either layer has a pixel, and no pixel (alpha) where both layers have a pixel (|frei0r.alphaxor|)"
msgstr ""
