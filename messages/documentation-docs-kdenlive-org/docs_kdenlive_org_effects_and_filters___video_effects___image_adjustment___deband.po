#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:44
msgid "This effect/filter removes banding artifacts from input video. It works by replacing banded pixels with average value of referenced pixels."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:48
msgid "Parameters"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:55
msgid "Parameter"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:56
msgid "Value"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:58
msgid "1st / 2nd / 3rd / 4th plane threshold"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:59
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:65
msgid "Float"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:60
msgid "Sets banding detection threshold for each :term:`plane`. Default is 0.02. Valid range is 0.00003 to 0.5. If difference between current pixel and reference pixel is less than threshold, it will be considered as banded."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:61
msgid "Range"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:62
msgid "Integer"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:63
msgid "Banding detection range in pixels. Default is 16. If positive, random number in range 0 to set value will be used. If negative, exact absolute value will be used. The range defines square of four pixels around current pixel."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:64
msgid "Direction"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:66
msgid "Sets direction in radians from which four pixel will be compared. If positive, random direction from 0 to set direction will be picked. If negative, exact of absolute value will be picked. For example direction 0, -PI or -2*PI radians will pick only pixels on same row and -PI/2 will pick only pixels on same column"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:67
msgid "Blur"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:68
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:71
msgid "Switch"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:69
msgid "If enabled, current pixel is compared with average value of all four surrounding pixels. The default is enabled. If disabled current pixel is compared with all four surrounding pixels. The pixel is considered banded if only all four differences with surrounding pixels are less than threshold."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:70
msgid "Coupling"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/image_adjustment/deband.rst:72
msgid "If enabled, current pixel is changed if and only if all pixel components are banded, e.g. banding detection threshold is triggered for all color components. The default is disabled."
msgstr ""

