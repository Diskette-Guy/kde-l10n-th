#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:62
msgid "Set the :term:`hue` value which will be used in color difference calculation. Allowed range is from -360.000 to 360.000. Default value is 0.000."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:63
msgid "av.sat"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:65
msgid "Set the :term:`saturation` value which will be used in color difference calculation. Allowed range is from -1.000 to 1.000. Default value is 0.000."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:66
msgid "av.val"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:68
msgid "Set the value which will be used in color difference calculation. Allowed range is from -1.000 to 1.000. Default value is 0.000."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:69
msgid "av.similarity"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:71
msgid "Set similarity percentage with the key color. Allowed range is from 0.000 to 1.000. Default value is 0.010.  0.001 matches only the exact key color, while 1.000 matches everything."
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:72
msgid "av.blend"
msgstr ""

#. +> trunk5
#: ../../effects_and_filters/video_effects/misc/hsvkey.rst:74
msgid "Blend percentage. Allowed range is from 0.000 to 1.000. Default value is 0.000. 0.000 makes pixels either fully gray, or not gray at all. Lower values result in more gray pixels, with a higher value the more similar the pixels color is to the key color."
msgstr ""

